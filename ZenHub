local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")

local scripts = {
    [118915549367482] = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/pp0D4Nrz", true))()
    end,
    [137925884276740] = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/AQ985E45", true))()
    end,
}

local currentPlace = game.PlaceId
local scriptFunc = scripts[currentPlace]

local function notify(title, text, duration)
    StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration or 5
    })
end

if scriptFunc then
    notify("Zen Hub", "Loading...", 5)
    local success, result = pcall(scriptFunc)
    if not success then
        notify("Zen Hub", "Error loading script!", 5)
    end
else
    notify("Zen Hub", "not tuff", 5)
    task.wait(2)
    Players.LocalPlayer:Kick("Game not supported!")
end
